Table ParkAreas {
    id integer [PK]
    areaName varchar
    location varchar
}

Table Services {
    id integer [PK]
    serviceName varchar
}

Table Guests {
    id integer [PK]
    firstName varchar
    lastName varchar
}

Table AreaServices {
    areaServices integer [PK]
    serviceId integer
    areaId integer
}

Table AreaGuest {
    areaGuestId integer [PK]
    areaId integer
    guestId integer
}

Ref: Services.id < AreaServices.serviceId  
Ref: ParkAreas.id < AreaServices.areaId

Ref: ParkAreas.id < AreaGuest.areaId
Ref: Guests.id < AreaGuest.guestId